# React App Environment Variables
# Copy this file to .env and modify as needed

# Backend API URL (Flask-SocketIO server)
# For development with webpack proxy, leave empty or use http://localhost:5000
# For production, set to your production backend URL
REACT_APP_API_URL=http://localhost:5000

# Webpack Dev Server Port (optional, default: 8080)
PORT=8080

# Auto-open browser on start (optional, default: true)
OPEN_BROWSER=true
